{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"declaration": true,
		"noEmit": false,
		"noImplicitAny": false,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"outDir": "./dist",
		"baseUrl": "./",
		"incremental": true,
		"strictNullChecks": false
	}
}
