{
	"name": "api",
	"version": "0.1.0",
	"private": true,
	"description": "",
	"author": "",
	"license": "UNLICENSED",
	"scripts": {
		"dev": "NODE_ENV=local nest start --watch",
		"prebuild": "rimraf dist",
		"build": "nest build",
		"prod": "node dist/main",
		"eslint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"eslint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"prettier": "prettier \"src/**/*.ts\" \"test/**/*.ts\"",
		"prettier:fix": "prettier \"src/**/*.ts\" \"test/**/*.ts\" --write",
		"lint": "yarn eslint && yarn prettier",
		"lint:fix": "yarn eslint:fix && yarn prettier:fix",
		"test": "yarn test:unit && yarn test:e2e",
		"test:unit": "jest --config ./test/jest-unit-test.config.ts",
		"test:unit:watch": "jest --config ./test/jest-unit-test.config.ts --watch",
		"test:unit:cov": "jest --config ./test/jest-unit-test.config.ts --coverage",
		"test:e2e": "jest --config ./test/jest-e2e-test.config.ts",
		"type-check": "tsc --noEmit",
		"verify": "concurrently \"yarn lint\" \"yarn type-check\""
	},
	"dependencies": {
		"@nestjs/common": "8.3.1",
		"@nestjs/config": "1.2.0",
		"@nestjs/core": "8.3.1",
		"@nestjs/graphql": "10.0.4",
		"@nestjs/apollo": "10.0.4",
		"@nestjs/passport": "8.2.1",
		"@nestjs/platform-express": "8.3.1",
		"@types/passport-jwt": "3.0.6",
		"apollo-server-express": "3.6.3",
		"class-transformer": "0.5.1",
		"class-validator": "0.13.2",
		"cookie-parser": "1.4.6",
		"envsub": "4.0.7",
		"express-openid-connect": "2.7.0",
		"js-yaml": "4.1.0",
		"jwks-rsa": "2.0.5",
		"jwt-decode": "3.1.2",
		"passport": "0.5.2",
		"passport-jwt": "4.0.0",
		"reflect-metadata": "0.1.13",
		"rimraf": "3.0.2",
		"rxjs": "7.5.2"
	},
	"devDependencies": {
		"@nestjs/cli": "8.2.0",
		"@nestjs/schematics": "8.0.5",
		"@nestjs/testing": "8.2.6",
		"@types/cookie-parser": "1.4.2",
		"@types/express": "4.17.13",
		"@types/js-yaml": "4.0.5",
		"@types/supertest": "2.0.11",
		"@types/validator": "13.7.1",
		"source-map-support": "0.5.21",
		"supertest": "6.2.2",
		"ts-jest": "27.1.3",
		"ts-loader": "9.2.6",
		"ts-node": "10.5.0",
		"tsconfig-paths": "3.12.0"
	},
	"peerDependencies": {
		"typescript": "*",
		"@types/node": "*",
		"@types/jest": "*",
		"eslint": "*",
		"jest": "*",
		"@typescript-eslint/eslint-plugin": "*",
		"@typescript-eslint/parser": "*",
		"eslint-config-prettier": "*",
		"eslint-plugin-prettier": "*",
		"prettier": "*",
		"graphql": "*"
	}
}
